#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/david/ddd84b1c-eeac-48db-804b-1d865cb4fb331/projects/pupcodeTutorial/api/node_modules/.pnpm/create-next-app@15.3.4/node_modules/create-next-app/dist/node_modules:/media/david/ddd84b1c-eeac-48db-804b-1d865cb4fb331/projects/pupcodeTutorial/api/node_modules/.pnpm/create-next-app@15.3.4/node_modules/create-next-app/node_modules:/media/david/ddd84b1c-eeac-48db-804b-1d865cb4fb331/projects/pupcodeTutorial/api/node_modules/.pnpm/create-next-app@15.3.4/node_modules:/media/david/ddd84b1c-eeac-48db-804b-1d865cb4fb331/projects/pupcodeTutorial/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/david/ddd84b1c-eeac-48db-804b-1d865cb4fb331/projects/pupcodeTutorial/api/node_modules/.pnpm/create-next-app@15.3.4/node_modules/create-next-app/dist/node_modules:/media/david/ddd84b1c-eeac-48db-804b-1d865cb4fb331/projects/pupcodeTutorial/api/node_modules/.pnpm/create-next-app@15.3.4/node_modules/create-next-app/node_modules:/media/david/ddd84b1c-eeac-48db-804b-1d865cb4fb331/projects/pupcodeTutorial/api/node_modules/.pnpm/create-next-app@15.3.4/node_modules:/media/david/ddd84b1c-eeac-48db-804b-1d865cb4fb331/projects/pupcodeTutorial/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.js" "$@"
else
  exec node  "$basedir/../../dist/index.js" "$@"
fi
